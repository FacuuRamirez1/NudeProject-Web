{
  "kind": "collectionType",
  "collectionName": "faq_items",
  "info": {
    "singularName": "faq-item",
    "pluralName": "faq-items",
    "displayName": "FaqItems"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "faqArticles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq-article.faq-article",
      "mappedBy": "faqItems"
    }
  }
}
